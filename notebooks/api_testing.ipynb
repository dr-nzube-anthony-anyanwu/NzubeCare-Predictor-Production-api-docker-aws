    "def test_api_health():\n",
    "    \"\"\"Test the API health endpoint.\"\"\"\n",
    "    try:\n",
    "        response = requests.get(f\"{API_BASE_URL}/health\", timeout=TIMEOUT)\n",
    "        print(f\"Status Code: {response.status_code}\")\n",
    "        \n",
    "        if response.status_code == 200:\n",
    "            health_data = response.json()\n",
    "            print(\"‚úÖ API is healthy!\")\n",
    "            print(f\"üìä Response: {json.dumps(health_data, indent=2)}\")\n",
    "            return True, health_data\n",
    "        else:\n",
    "            print(f\"‚ùå API health check failed: {response.text}\")\n",
    "            return False, None\n",
    "            \n",
    "    except requests.exceptions.RequestException as e:\n",
    "        print(f\"‚ùå Error connecting to API: {e}\")\n",
    "        print(\"\\nüí° Troubleshooting tips:\")\n",
    "        print(\"   1. Make sure the API server is running\")\n",
    "        print(\"   2. Check if the URL is correct\")\n",
    "        print(\"   3. Verify network connectivity\")\n",
    "        return False, None\n",
    "\n",
    "# Test the API\n",
    "api_healthy, health_info = test_api_health()"
   ]
