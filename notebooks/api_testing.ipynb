    "if 'prediction_results' in locals() and prediction_results:\n",
    "    # Prepare data for visualization\n",
    "    patient_types = list(prediction_results.keys())\n",
    "    risk_probabilities = [prediction_results[pt]['risk_probability'] for pt in patient_types]\n",
    "    risk_levels = [prediction_results[pt]['risk_level'] for pt in patient_types]\n",
    "    \n",
    "    # Create comprehensive visualization\n",
    "    fig = make_subplots(\n",
    "        rows=2, cols=2,\n",
    "        subplot_titles=('Risk Probabilities', 'Risk Level Distribution', \n",
    "                       'Patient Comparison', 'Feature Importance'),\n",
    "        specs=[[{\"type\": \"bar\"}, {\"type\": \"pie\"}],\n",
    "               [{\"type\": \"scatter\"}, {\"type\": \"bar\"}]]\n",
    "    )\n",
    "    \n",
    "    # 1. Risk probabilities bar chart\n",
    "    colors = ['red' if level == 'High' else 'orange' if level == 'Medium' else 'green' \n",
    "              for level in risk_levels]\n",
    "    \n",
    "    fig.add_trace(\n",
    "        go.Bar(x=patient_types, y=risk_probabilities, \n",
    "               marker_color=colors, name='Risk Probability',\n",
    "               text=[f'{p:.1%}' for p in risk_probabilities],\n",
    "               textposition='auto'),\n",
    "        row=1, col=1\n",
    "    )\n",
    "    \n",
    "    # 2. Risk level pie chart\n",
    "    risk_counts = pd.Series(risk_levels).value_counts()\n",
    "    fig.add_trace(\n",
    "        go.Pie(labels=risk_counts.index, values=risk_counts.values,\n",
    "               marker_colors=['green', 'orange', 'red'],\n",
    "               name='Risk Levels'),\n",
    "        row=1, col=2\n",
    "    )\n",
    "    \n",
    "    # 3. Patient comparison scatter\n",
    "    ages = [sample_patients[pt]['age_num'] for pt in patient_types]\n",
    "    hospital_days = [sample_patients[pt]['time_in_hospital'] for pt in patient_types]\n",
    "    \n",
    "    fig.add_trace(\n",
    "        go.Scatter(x=ages, y=hospital_days, \n",
    "                  mode='markers+text',\n",
    "                  marker=dict(size=[p*50 for p in risk_probabilities],\n",
    "                            color=risk_probabilities,\n",
    "                            colorscale='RdYlGn_r',\n",
    "                            showscale=True),\n",
    "                  text=patient_types,\n",
    "                  textposition='top center',\n",
    "                  name='Patients'),\n",
    "        row=2, col=1\n",
    "    )\n",
    "    \n",
    "    # 4. Feature importance for high-risk patient\n",
    "    if 'High Risk Patient' in prediction_results:\n",
    "        feature_imp = prediction_results['High Risk Patient']['feature_importance']\n",
    "        if isinstance(feature_imp, dict):\n",
    "            # Get top 10 features\n",
    "            sorted_features = sorted(feature_imp.items(), key=lambda x: x[1], reverse=True)[:10]\n",
    "            if sorted_features:\n",
    "                features, importances = zip(*sorted_features)\n",
    "                fig.add_trace(\n",
    "                    go.Bar(y=features, x=importances, orientation='h',\n",
    "                          name='Feature Importance'),\n",
    "                    row=2, col=2\n",
    "                )\n",
    "    \n",
    "    # Update layout\n",
    "    fig.update_layout(\n",
    "        title_text=\"NzubeCare API - Prediction Results Dashboard\",\n",
    "        height=800,\n",
    "        showlegend=False\n",
    "    )\n",
    "    \n",
    "    # Update axes labels\n",
    "    fig.update_xaxes(title_text=\"Patient Type\", row=1, col=1)\n",
    "    fig.update_yaxes(title_text=\"Risk Probability\", row=1, col=1)\n",
    "    fig.update_xaxes(title_text=\"Age\", row=2, col=1)\n",
    "    fig.update_yaxes(title_text=\"Hospital Days\", row=2, col=1)\n",
    "    fig.update_xaxes(title_text=\"Importance\", row=2, col=2)\n",
    "    \n",
    "    fig.show()\n",
    "    \n",
    "    # Summary statistics\n",
    "    print(\"\\nüìä Summary Statistics:\")\n",
    "    print(f\"   Average Risk Probability: {np.mean(risk_probabilities):.3f}\")\n",
    "    print(f\"   Risk Range: {min(risk_probabilities):.3f} - {max(risk_probabilities):.3f}\")\n",
    "    print(f\"   Most Common Risk Level: {pd.Series(risk_levels).mode()[0]}\")\n",
    "    \n",
    "else:\n",
    "    print(\"‚ùå No prediction results to visualize\")"
