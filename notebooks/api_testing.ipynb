    "def predict_single_patient(patient_data, patient_name=\"Patient\"):\n",
    "    \"\"\"Make a prediction for a single patient.\"\"\"\n",
    "    try:\n",
    "        response = requests.post(\n",
    "            f\"{API_BASE_URL}/predict\",\n",
    "            json=patient_data,\n",
    "            headers={\"Content-Type\": \"application/json\"},\n",
    "            timeout=TIMEOUT\n",
    "        )\n",
    "        \n",
    "        if response.status_code == 200:\n",
    "            result = response.json()\n",
    "            print(f\"‚úÖ {patient_name} Prediction Successful!\")\n",
    "            print(f\"   üéØ Risk Level: {result['risk_level']}\")\n",
    "            print(f\"   üìä Risk Probability: {result['risk_probability']:.1%}\")\n",
    "            print(f\"   üí° Recommendations: {len(result['recommendations'])} items\")\n",
    "            return True, result\n",
    "        else:\n",
    "            print(f\"‚ùå {patient_name} Prediction Failed: {response.status_code}\")\n",
    "            print(f\"   Error: {response.text}\")\n",
    "            return False, None\n",
    "            \n",
    "    except requests.exceptions.RequestException as e:\n",
    "        print(f\"‚ùå Error predicting for {patient_name}: {e}\")\n",
    "        return False, None\n",
    "\n",
    "# Test predictions for all sample patients\n",
    "if api_healthy:\n",
    "    prediction_results = {}\n",
    "    \n",
    "    for patient_type, patient_data in sample_patients.items():\n",
    "        print(f\"\\n{'='*50}\")\n",
    "        print(f\"Testing: {patient_type}\")\n",
    "        print(f\"{'='*50}\")\n",
    "        \n",
    "        success, result = predict_single_patient(patient_data, patient_type)\n",
    "        if success:\n",
    "            prediction_results[patient_type] = result\n",
    "            \n",
    "            # Display detailed results\n",
    "            print(f\"\\nüìã Detailed Results for {patient_type}:\")\n",
    "            print(f\"   Risk Probability: {result['risk_probability']:.3f}\")\n",
    "            print(f\"   Risk Level: {result['risk_level']}\")\n",
    "            print(f\"   Model Version: {result['model_version']}\")\n",
    "            \n",
    "            print(f\"\\nüí° Top Recommendations:\")\n",
    "            for i, rec in enumerate(result['recommendations'][:3], 1):\n",
    "                print(f\"   {i}. {rec}\")\n",
    "else:\n",
    "    print(\"‚ùå Cannot test predictions - API is not healthy\")"
   ]
